    <Resource Include="Assets\votesPNG.png" />
    <Resource Include="Assets\votesPNG1.png" />
    <Resource Include="Assets\votesPNG2.png" />
    <Resource Include="Assets\votesPNG4.png" />
  </ItemGroup>

</Project>
